#!/bin/sh -f

if test "$ELMER_LIB" = ""; then
  if  test "$ELMER_HOME" = ""; then
     INCLUDE=@prefix@/share/elmersolver/include
  else
     INCLUDE=$ELMER_HOME/share/elmersolver/include
  fi
else
  INCLUDE=$ELMER_LIB/../include
fi

cmd="@SH_LD@ @SH_LDFLAGS@ @SH_LINKING_TO_FLAGS@ @B64FLAGS@ @EXTRA_LIBS@ $*"
printf "%s " $cmd
printf "\n"
@SH_LD@ @SH_LDFLAGS@ @SH_LINKING_TO_FLAGS@ @B64FLAGS@ @EXTRA_LIBS@ $*

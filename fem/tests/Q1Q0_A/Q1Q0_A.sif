Check Keywords "Warn"

Header
  Mesh DB "." "Step"
End

Simulation
  Max Output Level = 3
  Coordinate System = "Cartesian 2D"
  Simulation Type = Steady
  Output Intervals(1) = 1
  Steady State Max Iterations = 100
  Post File   = "ptest.ep"
! Output File = "ptest.dat"
End

Body 1
  Equation = 1
  Material = 1
End

Equation 1 :: Active Solvers(1) = 1

Material 1
  Density   = 1
  Viscosity = 0.05
End
 
Solver 1
  Equation = "Stokes"

  Variable = Q1Q0[Velocity:2]
  Procedure = "Q1Q0_A" "StokesSolver"

  Exported Variable 1 = aPressure
  Nonlinear System Max Iterations = 1

  Nonlinear System Relaxation Factor = 1
  Steady State Convergence Tolerance = 1e-5
  
  Linear System Solver = Direct
  Linear System Direct Method = umfpack
End

Boundary Condition 1
  Target Boundaries(1) = 1
  Velocity 1 = Real 1
  Velocity 2 = Real 0
End

Boundary Condition 2
  Target Boundaries(1) = 2
  Velocity 2 = Real 0
End

Boundary Condition 3
  Target Boundaries(1) = 3
  Velocity 1 = Real 0
  Velocity 2 = Real 0
End

$fprintf( stderr, "TEST CASE 1\n");
RUN
$fprintf( stderr, "END TEST CASE 1: Target NRM=0.39427023\n" );


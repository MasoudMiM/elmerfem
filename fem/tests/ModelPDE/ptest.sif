Check Keywords "Warn"

Header :: Mesh DB "." "square"

Simulation
  Max Output Level = 9
  Coordinate System = Cartesian
  Simulation Type = Steady
  Output Intervals(1) = 1
  Steady State Max Iterations = 1
  Post File   = "ptest.ep"
End

Body 1
  Equation = 1
  Material = 1
  Body Force = 1
End

Material 1
  a 1 = Real 1
  a 2 = Real 0

  diffusion  coefficient = Real 1
  convection coefficient = Real 9
  time derivative coefficient = Real 0
End

Body Force 1 :: Source = Real 1
Equation 1 :: Active Solvers(1) = 1

Solver 1
  Equation = "Poisson"
  Variable = "Potential"
  Procedure = "AdvDiff" "AdvDiffSolver"
  Element = "p:2"
  Linear System Solver = Direct
  Steady State Convergence Tolerance = 1e-09
End

Boundary Condition 1
  Target Boundaries(1) = 1
  Potential = Real 0
End

Boundary Condition 2
  Target Boundaries(1) = 2
  coeff = Real 10
  ext_t = Real  5
End

Boundary Condition 3
  Target Boundaries(2) = 3 4
  flux = Real 10
End

$fprintf( stderr, "TEST CASE 1\n");
RUN
$fprintf( stderr, "END TEST CASE 1: Target NRM=1.0578593,EPS=1.0E-5\n" );

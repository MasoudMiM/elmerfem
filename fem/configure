static unsigned long int ulongval () { return (long int) (sizeof (off_t)); }
#include <stdio.h>
#include <stdlib.h>
#ifdef F77_DUMMY_MAIN

#  ifdef __cplusplus
     extern "C"
#  endif
   int F77_DUMMY_MAIN() { return 1; }

#endif

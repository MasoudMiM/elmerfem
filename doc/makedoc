#!/bin/sh -f
# Fetch sources from CVS, compile each module and install it
#
#export CVSROOT="username@corona.csc.fi:/fs/proj1/elmer/elmer-opensource"
#export CVS_RSH="ssh"

# replace these with your compilers:

modules="models tutorials elmersolver elmergrid cscmodels csctutorials elmerparam matc overview"
for m in $modules; do
  cd $m && make manual && make install && make clean && cd ..
done

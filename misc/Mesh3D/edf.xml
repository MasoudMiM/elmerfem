<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE edf>
<edf version="1.0">

  <PDE type="BuiltIn">
    <Name type="String"> Heat Equation </Name>

    <Solver>
      <Procedure type="File">
        <Name type="String"> HeatSolve </Name>
        <Function type="String"> HeatSolver </Function>
      </Procedure>
      <Variable type="Real"> Flow[velocity:3 pressure:1] </Variable>
    </Solver>

    <Material>
      <Parameter type="Real"> Heat Capacity </Parameter>
      <Parameter type="Real"> Heat Conductivity </Parameter>
    </Material>

    <BodyForce>
      <Parameter type="Real"> Heat Source </Parameter>
    </BodyForce>

    <Equation>
      <Parameter type="Combo" NameType="String" Name="Convection" SifName="Convection">
        <Entry type="Active"> None </Entry>
        <Entry type=""> Constant </Entry>
        <Entry type=""> Computed </Entry>
      </Parameter>	
    </Equation>

    <BoundaryCondition>
      <Parameter Type="Edit" NameType="Real" SifName="Variable"> $Variable </Parameter>
      <Parameter Type="Edit" NameType="Real" SifName="Heat Flux"> Heat Flux </Parameter>
      <Parameter Type="Edit" NameType="Real" SifName="External Temperature"> External Temperature </Parameter>
      <Parameter type="Edit" NameType="Real" SifName="Heat Transfer Coefficient"> Heat Transfer Coefficient </Parameter>
    </BoundaryCondition>		
  </PDE>

  <PDE type="BuiltIn">
    <Name type="String"> Linear elasticity </Name>
    <Active type="bool"> False </Active>

    <Solver>
      <Procedure type="File">
        <Name type="String"> StressSolve </Name>
        <Function type="String"> StressSolver </Function>
      </Procedure>

      <Parameter type="Real Vector(cdim)"> Displacement </Parameter>
    </Solver>

    <Material>
      <Parameter type="Real"> Youngs modulus </Parameter>
      <Parameter type="Real"> Poisson ratio </Parameter>
      <Parameter type="Real"> Heat expansion coefficient </Parameter>
    </Material>

    <BodyForce>
      <Function type="Real Vector(dim)"> Stress Bodyforce </Function>
    </BodyForce>

    <Equation>
      <Property type="Bool"> Plane stress </Property>	
    </Equation>

    <BoundaryCondition>
      <Parameter type="Real Vector(dim)"> Displacement </Parameter>
      <Parameter type="Real Vector"> Force </Parameter>
      <Parameter type="Real"> Normal Force </Parameter>
    </BoundaryCondition>		
  </PDE>
</edf>

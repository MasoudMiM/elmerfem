<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE edf>
<edf version="1.0">

  <PDE Widget="BuiltIn">
    <Name Widget="String"> Heat Equation </Name>

    <Solver>
      <Procedure Widget="File">
        <FileName Widget="String"> HeatSolve </FileName>
        <Function Widget="String"> HeatSolver </Function>
      </Procedure>
      <Variable Widget="Edit"> Temperature </Variable>
    </Solver>

    <Material>
      <Parameter Widget="Edit" SifName="Heat Capacity" Name="Heat Capacity" DefaultValue="1000"> </Parameter>
      <Parameter Widget="Edit" Name="Heat Conductivity" DefaultValue="1"> </Parameter>
      <Parameter Widget="Combo" Type="String" Name="Combo test" SifName="Convection">
        <Item> None </Item>
        <Item> Constant </Item>
        <Item Type="Active"> Computed </Item>
      </Parameter>	
      <Parameter Widget="CheckBox" Type="Logical" DefaultValue="true" Name="Logical test" SifName="some logical"> </Parameter>
    </Material>

    <BodyForce>
      <Parameter Widget="Edit"> Heat Source </Parameter>
    </BodyForce>

    <Equation>
      <Parameter Widget="Combo" Type="String" Name="Convection" SifName="Convection">
        <Item Widget="Active"> None </Item>
        <Item Widget=""> Constant </Item>
        <Item Widget=""> Computed </Item>
      </Parameter>	
    </Equation>

    <BoundaryCondition>
      <Parameter Widget="Edit" Type="Real" SifName="$Variable"> $Variable </Parameter>
      <Parameter Widget="Edit" Type="Real" SifName="Heat Flux"> Heat Flux </Parameter>
      <Parameter Widget="Edit" Type="Real" SifName="External Temperature"> External Temperature </Parameter>
      <Parameter Widget="Edit" Type="Real" SifName="Heat Transfer Coefficient"> Heat Transfer Coefficient </Parameter>
    </BoundaryCondition>		
  </PDE>

  <PDE Widget="BuiltIn">
    <Name Widget="String"> Linear elasticity </Name>
    <Active Widget="bool"> False </Active>

    <Solver>
      <Procedure Widget="File">
        <FileName Widget="String"> StressSolve </FileName>
        <Function Widget="String"> StressSolver </Function>
      </Procedure>

      <Parameter Widget="Real Vector(cdim)"> Displacement </Parameter>
    </Solver>

    <Material>
      <Parameter Widget="Real"> Youngs modulus </Parameter>
      <Parameter Widget="Real"> Poisson ratio </Parameter>
      <Parameter Widget="Real"> Heat expansion coefficient </Parameter>
    </Material>

    <BodyForce>
      <Function Widget="Real Vector(dim)"> Stress Bodyforce </Function>
    </BodyForce>

    <Equation>
      <Property Widget="Bool"> Plane stress </Property>	
    </Equation>

    <BoundaryCondition>
      <Parameter Widget="Real Vector(dim)"> Displacement </Parameter>
      <Parameter Widget="Real Vector"> Force </Parameter>
      <Parameter Widget="Real"> Normal Force </Parameter>
    </BoundaryCondition>		
  </PDE>
</edf>
